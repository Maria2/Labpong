<Window x:Class="LabPong.PreviousGames"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PreviousGames" Background="Black" Style="{DynamicResource window_template}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.1*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="0.1*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" FontSize="30" HorizontalAlignment="Center" Content="Last x Games" Name="title"/>
        <StackPanel Grid.Row="1">
            <ListBox Name="highscore" FontSize="20" Background="Black" Foreground="Red" BorderThickness="0">
                
            </ListBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Grid.Row="2">
            <Button Name="back" Content="Back" FontSize="20" Style="{DynamicResource button_temp}"/>
        </StackPanel>
    </Grid>
</Window>
