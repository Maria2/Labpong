<Window x:Class="LabPong.PreviousGames"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PreviousGames" Style="{DynamicResource window_template}" MouseMove="Window_MouseMove">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.1*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="0.1*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Background="Black">
            <Label FontSize="30" HorizontalAlignment="Center" Content="Last x Games" Name="title"/>
        </StackPanel>
        <StackPanel Grid.Row="1">
            <ListBox Name="highscore" FontSize="20" Background="Black" Height="50" Foreground="Red" BorderThickness="0">
                
            </ListBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Grid.Row="2">
            <Button Name="back" Content="Back" FontSize="20" Style="{DynamicResource button_temp}" MouseEnter="back_MouseEnter" />
        </StackPanel>
        <Canvas Name="canvas">
            <Ellipse Style="{DynamicResource pointer_template}" Name="pointer"/>
        </Canvas>
    </Grid>
</Window>
